{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "React Boilerplate",
  "main": "./src/client/index.js",
  "scripts": {
    "start": "npm run build && npm run server",
    "build": "npm run build:client && npm run build:server -- --prod",
    "build:client": "webpack --config webpack.prod.js",
    "build:server": "webpack --config webpack.server.js",
    "dev": "webpack-dev-server --inline --colors --config webpack.dev.js",
    "server": "node dist/server.js",
    "server:dev": "npm run build:server && npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:erikmswan/inprofundis.git"
  },
  "author": "Erik Swan",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "express": "^4.16.4",
    "loglevel": "^1.6.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^4.6.5",
    "react-redux": "^5.0.7",
    "react-transition-group": "^2.5.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-first-router": "^2.0.1",
    "redux-first-router-link": "^1.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.0.5",
    "node-sass": "^4.7.2",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.7.2"
  }
}
